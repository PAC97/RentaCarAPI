@model IEnumerable<RentaCars.EN.TipoUsuario>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<div class="row">
    <div class="col-md-6">
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
    </div>
    <div class="col-md-6">
        <p>
            @Html.ActionLink("Inactivos", "Index", new { state = ViewBag.Inactivo })
        </p>
    </div>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TipoUsuarioID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TipoUsuarioID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TipoUsuarioID })
        </td>
    </tr>
}

</table>
