@model IEnumerable<RentaCars.EN.Renta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UsuarioID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReservaID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstadoID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UsuarioID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReservaID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RentaID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RentaID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RentaID })
        </td>
    </tr>
}

</table>
